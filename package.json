{
  "name": "bookstore",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "build": "next build",
    "build-storybook": "storybook build",
    "check-types": "tsc --noemit",
    "db:seed": "dotenv -e .env.local -- prisma db seed",
    "db:reset": "dotenv -e .env.local -- prisma migrate reset",
    "dev": "next dev",
    "eslint": "next lint",
    "lint": "yarn eslint && yarn check-types",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
